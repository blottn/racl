{
	"manifest_version": 2,

	"name": "Racl",
	"description": "This is a really awesome crypto library!",
	"version": "1.0",
	"browser_action": {
		"default_title":"Racl",
		"default_popup":"popup.html"
	},
	"content_scripts": [
    {
      "matches": ["https://www.reddit.com/r/tcdcrypto/*/"],
	  
      "js": ["cryptico.min.js","racl.js"]
    }
  ],
	"permissions": [
		"http://*/",
		"activeTab",
		"storage"
	]
}
